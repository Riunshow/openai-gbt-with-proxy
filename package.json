{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "Openai-Gbt-With-Proxy",
  "version": "1.0.4",
  "title": "Openai-Gbt-With-Proxy",
  "description": "openai gbt with your own proxy",
  "icon": "openai-logo.png",
  "author": "Rainbower",
  "categories": [
    "Fun",
    "Web",
    "Applications"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "chat",
      "title": "chat",
      "subtitle": "Openai-Gbt-With-Proxy",
      "description": "openai gbt with your own proxy",
      "mode": "view",
      "preferences": [
        {
          "name": "openAiApiKey",
          "title": "OpenAI API Key",
          "description": "API Key is used to authenticate with OpenAI API",
          "type": "password",
          "required": true
        },
        {
          "name": "host",
          "title": "proxy host:port",
          "description": "your own proxy host:port",
          "type": "textfield",
          "required": false,
          "default": "127.0.0.1:7890"
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.48.9",
    "axios": "^1.3.4",
    "tunnel": "^0.0.6"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@types/tunnel": "^0.0.3",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}